#include <stdio.h>
int main()
{
    int a,b,i;
    b = 27;
    printf("Enter a number between 20 and 30\n");
    i = 0;
    do 
    {

        printf("Enter the number: ");
        scanf("%d", &a);
        i++;
    }
    while(a != b);
    {
        printf("Correct!");
        printf("\n%d attempts", i);
    }
    return 0;
}
  
