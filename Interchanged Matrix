#include <stdio.h>
int main()
{
	int a[3][3],i,j,r,c;
	printf("Enter the number of rows and columns: ");
	scanf("%d %d", &r,&c);
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			printf("Enter a[%d][%d]: ",i,j);
			scanf("%d", &a[i][j]);
		}
	}
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	}
	
	printf("\n");
	printf("Interchanged matrix");
	printf("\n____________________");
	printf("\n\n");
	
	for(i=0;i<r;i++)
	{
		for(j=0;j<c;j++)
		{
			printf("%d\t",a[j][i]);
		}
		printf("\n");	
	}
	return 0; 
}
