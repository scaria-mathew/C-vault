#include<stdio.h>
int main()
{
  int age;
  printf("Enter your age: ");
  scanf("%d", &age);
  
  printf("\n");
  
  if(age<=0)
  printf("Wrong input!");
  
  else if(age<=12)
  printf("Child");
  
  else if(age<=19)
  printf("Teenager");
  
  else if(age<60)
  printf("Adult");
  
  else if(age<=120)
  printf("Senior");
  
  return 0;
}
