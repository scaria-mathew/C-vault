#include<stdio.h>
int main()
{
  int m;
  printf("Enter the mark: ");
  scanf("%d", &m);
  
  if(m<0 || m>100)
  printf("Wrong input!");
 
  else if(m>=90 && m<=100)
  printf("A+");
  
  else if(m>=80)
  printf("A");
  
  else if(m>=70)
  printf("B");
  
  else if(m>=60)
  printf("C");
  
  else if(m>=50)
  printf("D");
  
  else if(m>=0)
  printf("F");

  return 0;
}
